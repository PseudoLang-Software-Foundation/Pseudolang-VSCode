{
  "name": "pseudolang-vscode",
  "displayName": "Pseudolang-VSCode",
  "description": "Adds support for Pseudolang",
  "version": "0.9.1",
  "publisher": "PseudolangSoftwareFoundation",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/PseudoLang-Software-Foundation/Pseudolang-VSCode.git"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pseudolang",
      "aliases": ["Pseudolang", "pseudolang"],
      "extensions": [".psl"],
      "icon": {
        "light": "./icons/psl-light.png",
        "dark": "./icons/psl-dark.png"
      },
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pseudolang",
      "scopeName": "source.pseudolang",
      "path": "./syntaxes/pseudolang.tmLanguage.json"
    }]
  }
}
